<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 必须使用此id,drawable为 未选中 状态的图片 -->
    <item
        android:id="@+id/background"
        android:drawable="@drawable/lib_style_star_all"/>
    <!-- 必须使用此id,drawable为 半选中 状态的图片 -->
    <item
        android:id="@+id/secondaryProgress"
        android:drawable="@drawable/lib_style_star_half"/>
    <!-- 必须使用此id,drawable为 全选中 状态的图片 -->
    <item
        android:id="@+id/progress"
        android:drawable="@drawable/lib_style_star_empty"/>
</layer-list>