<?xml version="1.0" encoding="utf-8"?>
<com.bwf.yiqizhuang.ui.initview.MyPullToRefresh xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/yiqiteam_fragment_refresh"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.bwf.yiqizhuang.ui.initview.MyScrollView
        android:id="@+id/yiqiteam_fragment_scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:descendantFocusability="blocksDescendants">

            <android.support.v7.widget.RecyclerView
                android:id="@+id/yiqiteam_fragment_recyclerView"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </android.support.v7.widget.RecyclerView>
        </RelativeLayout>
    </com.bwf.yiqizhuang.ui.initview.MyScrollView>

</com.bwf.yiqizhuang.ui.initview.MyPullToRefresh>